  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug export",
      "cargo": {
        "args": [
          "build",
          "--bin=heros_spirit_utils",
          "--package=heros_spirit_utils"
        ],
        "filter": {
          "name": "heros_spirit_utils",
          "kind": "bin"
        }
      },
      "args": ["export"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug import",
      "cargo": {
        "args": [
          "build",
          "--bin=heros_spirit_utils",
          "--package=heros_spirit_utils"
        ],
        "filter": {
          "name": "heros_spirit_utils",
          "kind": "bin"
        }
      },
      "args": ["import"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'heros_spirit_utils'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=heros_spirit_utils",
          "--package=heros_spirit_utils"
        ],
        "filter": {
          "name": "heros_spirit_utils",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
